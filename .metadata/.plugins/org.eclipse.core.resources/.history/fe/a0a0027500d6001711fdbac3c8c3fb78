
public class YourSchedule {
	public static void main(String[] args)
	{
		String p1, p2, p3, p4, t1, t2, t3, t4;
		p1 = "English";
		p2 = "ECS";
		p3 = "Math 2H";
		p4 = "Biology";
		t1 = "Myers";
		t2 = "Mauro";
		t3 = "Happ";
		t4 = "Balch";
		
		System.out.println("---------------------------------------------");
		System.out.println("|Per 1	|	"+p1+"	|	"+t1+"		|");
		System.out.println("|Per 2	|	"+p2+"	|	"+t2+"		|");
		System.out.println("|Per 3	|	"+p3+"	|	"+t3+"		|");
		System.out.println("|Per 4	|	"+p4+"	|	"+t4+"		|");
		System.out.println("---------------------------------------------");
	}

}
